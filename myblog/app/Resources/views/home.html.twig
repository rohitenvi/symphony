$('#datesorter').change(function() {

var order = $(this).val();
var sorted = $('.panel-body').sort(function(a, b) {


var a = new Date($(a).attr('data-id').split('/').join(' '));
var b = new Date($(b).attr('data-id').split('/').join(' '));
return order == 'desc' ?  b - a : a - b;
});

$('.sortedata').html(sorted);
});



